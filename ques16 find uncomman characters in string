public class Check{

	public static void main(String[] args) {

        int count=0;
		String s="tanya";
		char[] c=s.toCharArray();
          
          for(int i=0;i<=c.length-1;i++)
          {
          	count=1;
          	for(int j=i+1;j<=c.length-1;j++){
          		if(c[j]==c[i]){
          			count++;
          			c[j]='\0';
          		}
          	}
          	if(count==1 && c[i]!='\0')
          	System.out.println(c[i]+" = "+count+" times ");
          }
        
}
}
 
