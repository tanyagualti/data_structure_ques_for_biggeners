
<------------------------------------first method------------------------------------>
public class Check{

public static void main(String[] args) {
		 
      int[] arr={3,20,67,23};

       int max=arr[0];

       for(int i=1;i<=arr.length-1;i++){
       	if(arr[i]>max){
       	 max=arr[i];
       	}
       	
       }
    System.out.println(max);

    }}
